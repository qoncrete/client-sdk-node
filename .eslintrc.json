{
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "never"],
        "no-console": 0,
        "dot-location": [2, "object"],
        "dot-notation": 2,
        "newline-after-var": [2, "always"],
        "max-nested-callbacks": [2, 3],
        "max-params": [2, 4],
        "max-depth": [2, 3],
        "accessor-pairs": [2, { "setWithoutGet": true }],
        "block-scoped-var": 2,
        "complexity": [2, 7],
        "eqeqeq": 2,
        "no-eq-null": 2,
        "no-else-return": 2,
        "no-empty-pattern": 2,
        "no-eval": 2,
        "no-loop-func": 2,
        "no-lone-blocks": 2,
        "no-labels": 2,
        "yoda": 2,
        "wrap-iife": [2, "outside"],
        "radix": [2, "always"],
        "no-void": 2,
        "no-useless-call": 2,
        "no-self-compare": 2,
        "no-redeclare": 2,
        "no-octal": 2,
        "no-useless-concat": 2,
        "no-return-assign": 2,
        "no-multi-str": 2,
        "no-implied-eval": 2,
        "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true } }],
        "keyword-spacing": 2,
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "block-spacing": 2
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
